{
  "expo": {
    "name": "Chirp",
    "slug": "Chirp",
    "description": "Chirp",
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "./assets/chirp_icon.png",
    "splash": {
      "image": "./assets/chirp_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4c4c47"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.dylank.chirp",
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/chirp_android_icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/chirp_icon.png"
    }
  }
}
